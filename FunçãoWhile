import java.util.Scanner;

public class Main {

public static void main (String[] args) { 

    Scanner leitor = new Scanner(System.in);

 int i = 1;
        while (i <= 5) {
            System.out.println("Número: " + i);
            i++;  // Incrementa a variável i
        }
    }
}

// while: Verifica a condição antes de executar o código.
// Neste exemplo, o código imprime os números de 1 a 5. O loop continua enquanto a condição i <= 5 for verdadeira.
